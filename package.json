{
	"name": "@aloshai/mongosha",
	"version": "1.0.4",
	"author": {
		"name": "Alosha"
	},
	"description": "A fast, optimized and easy-to-use database manager. MongoDB has never been easier! Perfect for beginners!",
	"license": "GPL-3.0",
	"main": "dist",
	"keywords": [
		"mongosha",
		"mongodb",
		"mongoose",
		"quick-mongo",
		"mongo-fast",
		"mongo"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/aloshai/mongosha"
	},
	"typings": "dist",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"format": "prettier --write .",
		"format:watch": "onchange . -- prettier --write {{changed}}",
		"lint": "eslint --fix .",
		"lint:watch": "onchange . -- eslint --fix {{changed}}"
	},
	"devDependencies": {
		"@types/mongoose": "^5.10.3",
		"@types/node": "^14.14.16",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"@typescript-eslint/parser": "^4.11.0",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-import": "^2.22.1",
		"onchange": "^7.1.0",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"mongoose": "^5.11.8"
	}
}
